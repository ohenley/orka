with "orka_egl";

project Orka.Windows extends "orka" is

   OpenGL_Sources := OpenGL_Sources & "src/gl/windows";
   Orka_Sources   := Orka_Sources & "src/orka/windows";

   for Source_Dirs use OpenGL_Sources & Orka_Sources;

   for Object_Dir use "build/obj";
   for Library_Dir use "build/lib";

end Orka.Windows;
