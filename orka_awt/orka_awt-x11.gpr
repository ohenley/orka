with "orka_awt";
with "config/orka_awt_config";

project Orka_Awt.X11 is

   for Source_Dirs use ("src/x11", "src/shared", "src/shared/linux");

   for Object_Dir use "build/x11/obj/" & Orka_Awt_Config.Build_Profile;
   for Library_Dir use "build/x11/lib";

   package Compiler renames Orka_Awt.Compiler;
   package Binder   renames Orka_Awt.Binder;

   package Install is
      for Artifacts (".") use ("data");
   end Install;

end Orka_Awt.X11;